generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/.prisma/user-activities-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_USER_ACTIVITIES")
}

model Activity {
  id         Int       @id @default(autoincrement())
  userId     Int
  type       String
  date       DateTime
  createdBy  String?
  meta       Json?
  sessionEnd DateTime?
  sessionId  String?   @unique
}
