{
  "name": "dante-ai-data-agent-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "dev": "ts-node -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --ext .ts src packages",
    "lint:fix": "eslint --ext .ts src packages --fix",
    "format": "prettier --write 'src/**/*.ts' 'packages/**/*.ts'",
    "clean": "rimraf dist",
    "prisma:generate": "node scripts/generate-prisma-clients.js",
    "prisma:introspect": "npm run prisma:introspect:wallet && npm run prisma:introspect:bets && npm run prisma:introspect:activities && npm run prisma:introspect:finance",
    "prisma:introspect:wallet": "prisma db pull --schema=prisma/services/wallet.prisma",
    "prisma:introspect:bets": "prisma db pull --schema=prisma/services/bets-history.prisma",
    "prisma:introspect:activities": "prisma db pull --schema=prisma/services/user-activities.prisma",
    "prisma:introspect:finance": "prisma db pull --schema=prisma/services/financial-history.prisma",
    "prisma:migrate": "node scripts/create-prisma-migration.js",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma",
    "db:connect": "node -e \"const { setupDatabaseConnections } = require('./dist/packages/execution/database'); setupDatabaseConnections().then(() => console.log('Database connections established')).catch(err => console.error('Failed to connect to databases:', err))\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kashuta/chat-wit-sql.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kashuta/chat-wit-sql/issues"
  },
  "homepage": "https://github.com/kashuta/chat-wit-sql#readme",
  "description": "",
  "dependencies": {
    "@langchain/community": "^0.3.42",
    "@langchain/openai": "^0.5.10",
    "@prisma/client": "^6.8.0",
    "@types/node": "^22.15.18",
    "dotenv": "^16.5.0",
    "langchain": "^0.3.25",
    "prisma": "^6.8.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10"
  }
}
